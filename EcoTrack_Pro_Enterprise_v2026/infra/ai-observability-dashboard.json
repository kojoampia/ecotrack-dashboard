{
  "annotations": { "list": [] },
  "editable": true,
  "panels": [
    {
      "title": "Unit Economics: Cost per Carbon Calculation",
      "type": "stat",
      "gridPos": { "h": 6, "w": 24, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sum(rate(ai_cost_estimated_total[1h])) / sum(rate(carbon_calculation_total[1h]))",
          "legendFormat": "Global Avg Cost / Calculation"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "unit": "currencyUSD",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 0.01 },
              { "color": "red", "value": 0.05 }
            ]
          }
        }
      }
    },
    {
      "title": "Neural Analysis Latency",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
      "datasource": { "type": "tempo", "uid": "tempo" },
      "targets": [
        {
          "query": "{ .ai.model =~ \".*\" }",
          "queryType": "traceql"
        }
      ]
    },
    {
      "title": "Budget Breach Events (Tenant)",
      "type": "state-timeline",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sum(rate(ai_budget_exceeded_total[5m])) by (tenant_id)"
        }
      ]
    }
  ],
  "refresh": "10s",
  "schemaVersion": 36,
  "style": "dark",
  "title": "EcoTrackPro AI & Unit Economics",
  "uid": "ai-trace-map-v1"
}
